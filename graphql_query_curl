curl \
-X POST \
-H "Content-Type: application/json" \
--data '{ "query": "{ article(id: 1) { title } }" }' \
http://localhost:3000/graphql

curl \
-X POST \
-H "Content-Type: application/json"
--data '{ "query": "{ user(userInput:{first_name: \"test\"} ) {firstName lastName gender}}" }' \
http://localhost:3000/graphql
                                                                                                                                                 
